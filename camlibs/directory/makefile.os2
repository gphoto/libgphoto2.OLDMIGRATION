BASEPATH=..\..

all: start \
  dll \
  end

!include $(BASEPATH)\OS2MAK.INC

dll: $(CAMLIBS)\gpdirbrw.dll

OBJS=directory.o

.c.o:
   @$(CC) -o $@ $< $(CCFLAGS)

directory.def: directory.o
   @$(MAKEDEF)

$(CAMLIBS)\gpdirbrw.dll: $(OBJS) directory.def
   @$(CC) $(DLLLINKFLAGS) -o $@ $** $(LIBS) -lst\check -lgcc

