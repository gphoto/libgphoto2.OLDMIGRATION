msgstr "Impossible de verrouiller le périphérique"

#: libgphoto2_port/gphoto2-port-result.c:85
msgid "Unknown error"
msgstr "Erreur inconnue"

#: usb/libusb.c:131
#, c-format
msgid "Could not open USB device (%m)."
msgstr "Impossible d'ouvrir le périphérique USB (%m)."

#: usb/libusb.c:138
#, c-format
msgid ""
"Could not claim interface %d (%m). Make sure no other program or kernel "
"module (i.e. dc2xx) is using the device and you have read/write access to "
"the device."
msgstr ""
"Impossible d'utiliser l'interface %d (%m). Assurez-vous qu'aucun programme "
"ou module noyau (par ex. dc2xx) n'utilise le périphérique et que vous avez "
"accès en lecture/écriture au device."

#: usb/libusb.c:157
#, c-format
msgid "Could not release interface %d (%m)."
msgstr "Impossible de relâcher l'interface %d (%m)."

#: usb/libusb.c:164
#, c-format
msgid "Could not close USB port (%m)."
msgstr "Impossible de fermer le port USB (%m)."

#: usb/libusb.c:278
#, c-format
msgid "Could not set config %d/%d (%m)"
msgstr "Impossible de configurer %d/%d (%m)"

#: usb/libusb.c:300
#, fuzzy, c-format
msgid "Could not set altsetting %d/%d (%m)"
msgstr "Impossible de configurer %d/%d (%m)"

#: usb/libusb.c:374
#, c-format
msgid "The supplied vendor or product id (0x%x,0x%x) is not valid."
msgstr "Le couple fabricant/produit (0x%x,0x%x) est invalide."

#: usb/libusb.c:412
#, c-format
msgid ""
"Could not find USB device (vendor 0x%x, product 0x%x). Make sure this device "
"is connected to the computer."
msgstr ""
"Could not find USB device (vendor 0x%x, product 0x%x). Make sure this device "
"is connected to the computer."

#: usb/libusb.c:508
#, c-format
msgid ""
"Could not find USB device (class 0x%x, subclass 0x%x, protocol 0x%x). Make "
"sure this device is connected to the computer."
msgstr ""
"Could not find USB device (class 0x%x, subclass 0x%x, protocol 0x%x). Make "
"sure this device is connected to the computer."

#: serial/unix.c:190
#, c-format
msgid "Could not lock device '%s'"
msgstr "Impossible de verrouiller le périphérique '%s'"

#: serial/unix.c:199
#, c-format
msgid "Device '%s' is locked by pid %d"
msgstr "Le device '%s' est verrouillé par le processus %d."

#: serial/unix.c:202
#, c-format
msgid "Device '%s' could not be locked (dev_lock returned %d)"
msgstr "Le device '%s' ne peut être verrouillé (dev_unlock a retourné %d)"

#: serial/unix.c:224
#, c-format
msgid "Device '%s' could not be unlocked."
msgstr "Impossible de déverrouiller le device '%s'."

#: serial/unix.c:236
#, c-format
msgid "Device '%s' could not be unlocked as it is locked by pid %d."
msgstr ""
"Le device '%s' ne peut être déverrouillé car vérouillé par processus %d."

#: serial/unix.c:240
#, c-format
msgid "Device '%s' could not be unlocked (dev_unlock returned %d)"
msgstr "Le device '%s' ne peut être déverrouillé (dev_unlock a retourné %d)"

#: serial/unix.c:307
#, c-format
msgid "Serial Port %i"
msgstr "Port série %i"

#: serial/unix.c:389
#, c-format
msgid "Failed to open '%s' (%m)."
msgstr "Impossible d'ouvrir '%s' (%m.)"

#: serial/unix.c:407
#, c-format
msgid "Could not close '%s' (%m)."
msgstr "Impossible de fermer '%s' (%m.)"

#: serial/unix.c:452
#, c-format
msgid "Could not write to port (%m)"
msgstr "Impossible d'écrire sur le port (%)"

