#, c-format
msgid "Failed to open '%s' (%m)."
msgstr "Falhou ao abrir '%s' (%m)."

#: serial/unix.c:435
#, c-format
msgid "Could not close '%s' (%m)."
msgstr "Não pude fechar '%s' (%m)."

